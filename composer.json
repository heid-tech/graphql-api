  "minimum-stability": "stable",
  "type": "typo3-cms-extension",
  "license": "MIT",
  "version": "0.0.1",
  "authors": [
    {
      "name": "Rafael Heid",
      "email": "rafaelheid.it@gmail.com"
    }
